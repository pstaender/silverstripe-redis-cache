{
  "name": "pstaender/silverstripe-redis-cache",
  "description": "Enables Redis cache for SilverStripe",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/pstaender/silverstripe-redis-cache",
  "keywords": [
      "silverstripe",
      "redis",
      "cache"
  ],
  "license": "MIT",
  "support": {
      "issues": "https://github.com/pstaender/silverstripe-redis-cache/issues"
  },
  "authors": [
      {
          "name": "Philipp Staender",
          "email": "pstaender@mailbox.org"
      }
  ],
  "require": {
      "silverstripe/framework": "~4 || ^5",
      "predis/predis": "^2.1.2"
  },
  "require-dev": {
      "phpunit/phpunit": "~9.5"
  },
  "autoload": {
      "psr-4": {
          "Zeitpulse\\": "src/"
      }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
